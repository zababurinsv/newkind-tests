{
  "name": "@newkind/tests",
  "version": "0.3.0",
  "description": "Mocha tests",
  "source": "./frontend/src/index.mjs",
  "module": "./dist/index.mjs",
  "main": "./dist/index.mjs",
  "targets": {
    "dev": {
      "includeNodeModules": true,
      "optimize": true,
      "isLibrary": false,
      "distDir": "dist",
      "sourceMap": true,
      "publicUrl": "/",
      "source": "./frontend/src/index.html"
    },
    "service": {
      "includeNodeModules": true,
      "optimize": true,
      "isLibrary": false,
      "distDir": "dist",
      "sourceMap": false,
      "publicUrl": "/",
      "source": "./frontend/src/index.html"
    },
    "module": {
      "includeNodeModules": true,
      "optimize": true,
      "isLibrary": true,
      "sourceMap": false,
      "source": "./frontend/src/modules/index.mjs"
    }
  },
  "config": {
    "port": 4548,
    "gulp": {
      "scope": "/frontend/src"
    },
    "list": {
      "white": [
        "http://localhost:4519",
        "http://localhost:4517",
        "https://zababurinsv.github.io/newkind-service/",
        "http://localhost:4518/"
      ],
      "black": []
    }
  },
  "staticFiles": {
    "staticPath": "frontend/src/static",
    "distDir": "service"
  },
  "scripts": {
    "build:dev": "rimraf ./dist/* && parcel --cache-dir ./.parcel-cache --port $npm_package_config_port  --target dev",
    "build:service": "rimraf ./dist/* && rimraf ./.parcel-cache && parcel --cache-dir ./.parcel-cache --target service",
    "build:module": "rimraf ./dist/* && parcel build --target module",
    "build:chai": "rimraf ./frontend/src/modules/chai/* && parcel --dist-dir ./frontend/src/modules/chai --target module",
    "gulp:watch": "gulp",
    "nodemon:start": "nodemon ./backend/index.mjs",
    "kill": "kill $(lsof -t -i:$npm_package_config_port)",
    "clean:parcel": "find . -name \".parcel-cache\" -exec rm -rf '{}' +; ",
    "clean": "rimraf ./docs/* && rimraf ./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zababurinsv/newkind-tests.git"
  },
  "bugs": {
    "url": "https://github.com/zababurinsv/newkind-tests/issues"
  },
  "homepage": "https://github.com/zababurinsv/newkind-tests#readme",
  "keywords": [
    "task",
    "manager"
  ],
  "author": "Zababurin Sergey",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@newkind/events": "^0.3.0",
    "chai": "^4.3.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eruda-dom": "^2.0.0",
    "express": "^4.18.1",
    "express-enqueue": "^1.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "socket.io": "^4.5.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
