{
  "name": "@newkind/tests",
  "version": "0.1.5",
  "description": "Mocha tests",
  "source": "./frontend/src/index.mjs",
  "module": "./dist/index.mjs",
  "main": "./dist/index.mjs",
  "targets": {
    "main": false,
    "module": {
      "includeNodeModules": true,
      "optimize": true
    },
    "docs": {
      "source": "./frontend/src/index.html"
    }
  },
  "config": {
    "html": "./frontend/src/index.html",
    "mjs": "./frontend/src/index.mjs",
    "port": 8358
  },
  "scripts": {
    "dev": "parcel $npm_package_config_html --port $npm_package_config_port --cache-dir ./.parcel-cache  --no-source-maps --dist-dir ./docs ",
    "build:html": "rimraf ./docs/* && parcel build --target docs --no-source-maps --dist-dir ./docs ",
    "build": "rimraf ./dist/* && parcel build $npm_package_config_mjs --no-source-maps --dist-dir ./dist ",
    "kill": "kill $(lsof -t -i:$npm_package_config_port)",
    "clean:parcel": "find . -name \".parcel-cache\" -exec rm -rf '{}' +; ",
    "clean": "rimraf ./docs/* && rimraf ./dist/*",
    "npm:publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zababurinsv/newkind-tests.git"
  },
  "bugs": {
    "url": "https://github.com/zababurinsv/newkind-tests/issues"
  },
  "homepage": "https://github.com/zababurinsv/newkind-tests#readme",
  "keywords": [
    "task",
    "manager"
  ],
  "author": "Zababurin Sergey",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@newkind/tests": "0.1.5"
  }
}
